<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView_item_supplement_add_time_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        app:srcCompat="@drawable/nutrition_filled_minus" />

    <LinearLayout
        android:id="@+id/layout_item_supplement_add_time"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="6dp"
        android:layout_weight="1"
        android:background="@drawable/all_rect_gray2_r5"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView_item_supplement_add_time"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="21dp"
            android:fontFamily="@font/pretendard_medium"
            android:gravity="center_vertical"
            android:text="TextView"
            android:textColor="#444444"
            android:textSize="16sp" />
    </LinearLayout>
</LinearLayout>